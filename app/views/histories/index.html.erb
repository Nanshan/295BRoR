<h1>Listing histories</h1>

<table>
  <thead>
    <tr>
      <th>Userid</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Like</th>
      <th>CreatedTime</th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @histories.each do |history| %>
      <tr>
        <td><%= history.userId %></td>
        <td><%= history.latitude %></td>
        <td><%= history.longitude %></td>
        <td><%= history.like %></td>
        <td><%= history.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>


